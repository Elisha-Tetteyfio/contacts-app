<div id="<%= dom_id contact %>">
  <p>
    <strong>Fname:</strong>
    <%= contact.fname %>
  </p>

  <p>
    <strong>Lname:</strong>
    <%= contact.lname %>
  </p>

  <p>
    <strong>Phone:</strong>
    <%= contact.phone %>
  </p>

  <p>
    <strong>Birth date:</strong>
    <%= contact.birth_date %>
  </p>

  <p>
    <strong>Email:</strong>
    <%= contact.email %>
  </p>

  <p>
    <strong>User:</strong>
    <%= contact.user_id %>
  </p>

  <p>
    <strong>Suburb:</strong>
    <%= contact.suburb_id %>
  </p>

</div>
